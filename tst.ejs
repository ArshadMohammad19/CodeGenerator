    <style>
       .logo-container {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100vh;
  overflow: hidden;
}

.logo {
  position: absolute;
  top: 50%;
  left: -50px; 
  width: 50px; 
  transform: translateY(-50%);
  animation: moveRight 3s linear infinite;
}

@keyframes moveRight {
  0% {
    transform: translateX(-100%); 
  }
  100% {
    transform: translateX(100%); 
  }
}

    </style>
</head>
